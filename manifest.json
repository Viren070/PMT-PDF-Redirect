{
    "manifest_version": 3,
    "name": "PMT PDF Link Redirect",
    "version": "1.1",
    "description": "Remove PMT's useless PDF viewer which barely works and redirects you to the original pdf file without any ads",
    "permissions": ["webNavigation"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }